<!--XML最好存储为UTF-8编码-->
<xlue>

<objtreetemplate id="HelloBolt.Tree" class="ObjectTreeTemplate">
	<attr>
			<left>-200</left>
			<top>-200</top>
			<width>2000</width>
			<height>2000</height>
	</attr>

										
	<obj id="app.bkg" class="ImageObject">
		<attr>
			<image>bkg</image>
		</attr>
		<children>
			<obj id="caption" class="CaptionObject">
				<attr>
					<width>father.width</width>
					<height>father.height</height>
					<zorder>1000</zorder>
				</attr>
			</obj>
			
			<obj id="browser" class="BoltBrowserObject">
				<attr>
					<width>father.width</width>
					<height>father.height / 2</height>
					<popupinplace>0</popupinplace>
					<url>file://d:/dev/code_git/BXF/tests/xars/BoltBrowserObject/html/binding.html</url>
				</attr>
				<eventlist>
					<event name="OnLoadingStateChange" file="MainWnd.xml.lua" func="OnLoadingStateChange"/>
					<event name="OnBeforeNavigation" file="MainWnd.xml.lua" func="OnBeforeNavigation"/>
					<event name="OnBeforePopup" file="MainWnd.xml.lua" func="OnBeforePopup"/>
					<event name="OnJavaScriptMessageReceived" file="MainWnd.xml.lua" func="OnJavaScriptMessageReceived"/>
				</eventlist>
			</obj>
			
			<obj id="childbrowser" class="WindowlessBoltBrowserObject">
				<attr>
					<width>father.width</width>
					<top>father.height / 2</top>
					<height>father.height/2-50</height>
					<alpha>150</alpha>
				</attr>
				<eventlist>
				</eventlist>
			</obj>

			<obj id="boltuibkg" class="FillObject">
				<attr>
					<width>father.width</width>
					<top>father.height-50</top>
					<height>50</height>
					<filltype>Monochrome</filltype>
					<blendtype>Source</blendtype>
					<destcolor>0,0,0,255</destcolor>
					<srccolor>0,0,0,255</srccolor>
					<srcpt>0,0</srcpt>
					<destpt>width,height</destpt>
				</attr>
			</obj>
			<obj id="boltui" class="LayoutObject">
				<attr>
					<width>father.width</width>
					<top>father.height-50</top>
					<height>50</height>
				</attr>
				<children>
					<obj id="calljavascriptfunction" class="FillObject">
						<attr>
							<left>20</left>
							<top>10</top>
							<width>200</width>
							<height>father.height-20</height>
							<filltype>Monochrome</filltype>
							<blendtype>Source</blendtype>
							<destcolor>255,255,255,255</destcolor>
							<srccolor>255,255,255,255</srccolor>
							<srcpt>0,0</srcpt>
							<destpt>0,height</destpt>
						</attr>
						<children>
							<obj id="text.calljs" class="TextObject">
								<attr>
									<width>father.width</width>
									<height>father.height</height>
									<text>CallJavascriptFunction</text>
									<valign>center</valign>
									<halign>center</halign>
								</attr>
								<eventlist>
									<event name="OnLButtonDown" file="MainWnd.xml.lua" func="CallJavascriptButton_MouseLeftDown"/>
								</eventlist>
							</obj>
						</children>
					</obj>
					<obj id="sendmessage2javascript" class="FillObject">
						<attr>
							<left>20+220</left>
							<top>10</top>
							<width>200</width>
							<height>father.height-20</height>
							<filltype>Monochrome</filltype>
							<blendtype>Source</blendtype>
							<destcolor>255,255,255,255</destcolor>
							<srccolor>255,255,255,255</srccolor>
							<srcpt>0,0</srcpt>
							<destpt>0,height</destpt>
						</attr>
						<children>
							<obj id="text.sendmessage" class="TextObject">
								<attr>
									<width>father.width</width>
									<height>father.height</height>
									<text>Send Message 2 Javasctipt</text>
									<valign>center</valign>
									<halign>center</halign>
								</attr>
								<eventlist>
									<event name="OnLButtonDown" file="MainWnd.xml.lua" func="SendLuaMessage2JavascriptButton_MouseLeftDown"/>
								</eventlist>
							</obj>
						</children>
					</obj>
				</children>
			</obj>

			
		</children>
	</obj>
</objtreetemplate>
	
<hostwndtemplate id="HelloBolt.Wnd" class="FrameHostWnd">
		<attr> 
			<title>Bolt</title>
			<layered>1</layered>
			<left>200</left> 
			<top>100</top>
			<!--注意这是容器窗口的大小，设置的要合理-->
			<width>800</width>
			<height>600</height>
			<topmost>0</topmost>
			<visible>1</visible>
			<enable>1</enable>
			<maxbox>0</maxbox>
			<minbox>0</minbox>
			<minwidth>100</minwidth>
			<minheight>72</minheight>
			<maxwidth>1000</maxwidth>
			<maxheight>720</maxheight>
			<appwindow>1</appwindow>
			<fps>30</fps>
		</attr>
		<eventlist>
			<event name="OnSize" file="MainWnd.xml.lua" func="OnSize"/>
			<event name="OnClose" file="MainWnd.xml.lua" func="OnClose"/>
			<event name="OnCreate" file="MainWnd.xml.lua" func="OnCreate"/>
		</eventlist>
</hostwndtemplate>
</xlue>
